pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello World I am 1'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello World I am Hello 2'
            }
        }
        stage('Release') {
            steps {
                echo 'Hello World I am Hello 2'
            }
        }
    }
}
